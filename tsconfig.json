{
  "compilerOptions": {
    "target": "ES2022",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "outDir": "dist",
    "rootDir": "src",
    "esModuleInterop": true,
    "strict": true,
    "typeRoots": ["./src/types", "./node_modules/@types"],
    "skipLibCheck": true,
  },
  "include": ["src"]
}